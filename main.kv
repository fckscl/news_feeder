#:kivy 2.1
#:import MDFillRoundFlatButton kivymd.uix.button
#:import MDLabel kivymd.uix.label.MDLabel


Parse:

<News>:
    orientation: 'vertical'
    size_hint: 1, None
    height: self.minimum_height
    MDFillRoundFlatButton:
        id: "button_title"
        #size: '200dp', '200dp'
        size_hint: 1, None
        height: '48dp'
        text: root.t
        on_press: root.button_clicked()
    MDLabel:
        id: "label_text"
        size_hint: 1, None
        text_size: root.width, None
        size: self.texture_size
        text: root.parse

<Parse@ScrollView>:
    Stack:

<Stack>:
    orientation: 'vertical'
    size_hint: 0.9, None
    # pos_hint: 0.5, None
    height: self.minimum_height
    spacing: '10dp'
        